// Solicitar al usuario que ingrese un número
var numero = prompt("Ingresa un número:");

// Convertir el input en número
var numeroConvertido = parseInt(numero);

// Determinar si el número es divisible por 11 y 5
if (numeroConvertido % 11 === 0 && numeroConvertido % 5 === 0) {
    console.log(numeroConvertido + " es divisible por 11 y 5.");
} else {
    console.log(numeroConvertido + " no es divisible por 11 y 5 al mismo tiempo.");
}
