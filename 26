const numero = parseInt(prompt("Ingresa un número:"));

if (!isNaN(numero)) {
    console.log(`Los divisores de ${numero} son:`);

    for (let i = 1; i <= numero; i++) {
        if (numero % i === 0) {
            console.log(i);
        }
    }
} else {
    console.log("El valor ingresado no es un número válido.");
}
