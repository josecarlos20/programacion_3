// Crear un array vacío
let numerosAleatorios = [];

// Generar 20 números al azar y guardarlos en el array
for (let i = 0; i < 20; i++) {
  let numeroAleatorio = Math.floor(Math.random() * 100); // Cambia el rango si lo deseas
  numerosAleatorios.push(numeroAleatorio);
}

console.log(numerosAleatorios); // Mostrar el array de números aleatorios
